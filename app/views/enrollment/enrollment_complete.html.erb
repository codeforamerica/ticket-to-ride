<div class="enrollment-form">
  <div class="enrollment-form_para">Congratulations! You've completed the online portion of <span class="edited_text"><%= @student.first_name %></span>'s school registration.
  </div>

  <div><span class="instructions">Please bring the following documents with you to finalize your application:</span>

    <div class="row">
      <div class="col-lg-6">
        <div class="input-group">
          <span class="input-group-addon">
            <input type="checkbox">
          </span>
          <span class="form-control">Student birth certificate</span>
        </div>
        <!-- /input-group -->
      </div>
      <!-- /.col-lg-6 -->

      <div class="col-lg-6">
        <div class="input-group">
          <span class="input-group-addon">
            <input type="checkbox">
          </span>
          <span class="form-control">Student immunization form</span>
        </div>
        <!-- /input-group -->
      </div>
      <!-- /.col-lg-6 -->
    </div>

    <div class="row">
      <div class="col-lg-6">
        <div class="input-group">
          <span class="input-group-addon">
            <input type="checkbox">
          </span>
          <span class="form-control">Your state photo I.D.</span>
        </div>
        <!-- /input-group -->
      </div>
      <!-- /.col-lg-6 -->

      <div class="col-lg-6">
        <div class="input-group">
          <span class="input-group-addon">
            <input type="checkbox">
          </span>
          <span class="form-control">Proof of residence</span>
        </div>
        <!-- /input-group -->
      </div>
      <!-- /.col-lg-6 -->
    </div>
  </div>
  <%= form_for(@student, url: wizard_path, method: :put) do |s| %>
      <ul class="pager">
        <li><%= s.submit 'Finish', class: 'nav-button pull-right' %></li>
      </ul>
  <% end %>
</div>

<%= render 'shared/scenery_enrollment_complete' %>